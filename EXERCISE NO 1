import java.util.*;

public class main
 {
    public static void main(String[] args){ 
      Scanner obj = new Scanner (System.in);
      System.out.println("enter the number:");
      int num=obj.nextInt();
      System.out.println("num is"+num);
      if(num%2==0)
      {
      System.out.println("even");
      }
      else
      {
      System.out.println("odd");
      }
    }  
  }
